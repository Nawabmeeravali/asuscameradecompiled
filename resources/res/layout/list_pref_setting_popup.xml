<?xml version="1.0" encoding="utf-8"?>
<com.android.camera.p004ui.ListPrefSettingPopup xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" style="@style/SettingPopupWindow">
    <LinearLayout android:orientation="vertical" android:background="@color/popup_background" android:layout_width="@dimen/setting_popup_window_width" android:layout_height="wrap_content">
        <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content" android:minHeight="@dimen/popup_title_frame_min_height">
            <TextView android:id="@+id/title" style="@style/PopupTitleText"/>
        </FrameLayout>
        <View style="@style/PopupTitleSeparator"/>
        <FrameLayout android:layout_width="match_parent" android:layout_height="wrap_content">
            <ListView android:id="@+id/settingList" android:choiceMode="singleChoice" style="@style/SettingItemList"/>
        </FrameLayout>
    </LinearLayout>
</com.android.camera.p004ui.ListPrefSettingPopup>
